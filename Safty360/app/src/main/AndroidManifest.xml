<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jhtwl.safty360">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".SetingActivity" />
        <activity android:name=".PhoneTheft.PhoneTheftActivity" />
        <activity android:name=".PhoneTheft.BaseSettingGuidActivity" />
        <activity android:name=".PhoneTheft.SettingGuidActivity1" />
        <activity android:name=".PhoneTheft.SettingGuidActivity2" />
        <activity android:name=".PhoneTheft.SettingGuidActivity3" />
        <activity android:name=".PhoneTheft.SettingGuidActivity4" />
        <activity android:name=".PhoneTheft.ContactActivity" />
    </application>

</manifest>